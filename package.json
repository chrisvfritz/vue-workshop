{
  "appInfo": {
    "title": "Workshop: Proven patterns for building Vue apps",
    "description": "We'll cover everything you need to know to get started building world-class Vue applications. Topics will include configuring Webpack for single-file components, setting up the most advanced workflows currently possible, how to organize (and reorganize) increasingly complex applications, and more."
  },
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "unit": "jest --coverage --no-cache",
    "unit:watch": "jest --watch",
    "cypress:run": "cross-env CYPRESS_BASE_URL=http://localhost:9090 cypress run",
    "e2e": "start-server-and-test start http://localhost:9090 cypress:run",
    "test": "yarn unit && yarn e2e",
    "format": "cross-env NODE_ENV=production scripts/format.js",
    "format:fix": "yarn format --fix",
    "eslint": "cross-env NODE_ENV=production eslint --ext .js,.vue .",
    "eslint:fix": "eslint --ext .js,.vue --fix .",
    "stylelint": "cross-env NODE_ENV=production stylelint \"src/**/*.vue\" \"src/**/*.scss\" \"src/**/*.css\"",
    "stylelint:fix": "yarn stylelint --fix",
    "markdownlint": "markdownlint README.md scripts/ .vscode/ build/ config/ src/ static/ test/",
    "prelint": "yarn format",
    "lint": "yarn eslint && yarn stylelint && yarn markdownlint",
    "prelint:fix": "yarn format --fix",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix && yarn markdownlint",
    "build": "node build/build.js",
    "prestart": "yarn build",
    "start": "pushstate-server dist 9090"
  },
